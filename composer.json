{
    "name": "ayles-software/laravel-sms-clicksend",
    "description": "ClickSend Notifications channel for Laravel 8",
    "keywords": ["laravel", "notifications", "messagemedia", "sms"],
    "homepage": "https://github.com/parkourben99/laravel-sms-clicksend",
    "license": "MIT",
    "authors": [
        {
            "name": "Benjamin Ayles",
            "role": "Developer",
            "email": "ben@aylessoftware.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/events": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/notifications": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "AylesSoftware\\MessageMedia\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\ClickSend\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "cs-fix": "vendor/bin/php-cs-fixer fix"
    }
}
